# Bash completion script for jst

_jst()
{
    _script_commands="init build deploy bd start stop restart ps log go status update diff revert svn -h --help"
    
    local cur prev
    COMPREPLY=()
    cur="${COMP_WORDS[COMP_CWORD]}"
    COMPREPLY=( $(compgen -W "${_script_commands}" -- ${cur}) )
    return 0
}
complete -o nospace -o plusdirs -F _jst jst
